{
  "name": "@aimpact/airooms",
  "version": "1.0.0",
  "title": "AI Rooms",
  "description": "package to manage rooms",
  "license": "ISC",
  "layout": "main-layout",
  "template": "template/template.json",
  "languages": {
    "default": "en",
    "supported": [
      "en",
      "es"
    ]
  },
  "modules": {
    "path": "modules"
  },
  "deployment": {
    "distributions": [
      {
        "name": "web",
        "environment": "development",
        "platform": "web",
        "ports": {
          "bundles": 7070
        }
      }
    ]
  },
  "dependencies": {
    "@beyond-js/kernel": "~0.1.8",
    "@beyond-js/local": "~0.1.3",
    "@beyond-js/react-18-widgets": "~0.0.1",
    "socket.io-client": "^4.5.4",
    "@beyond-js/reactive": "~1.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9"
  }
}
